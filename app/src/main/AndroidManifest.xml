<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vocabwin5.vocabwin5">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.vocabwin5.vocabwin5.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vocabwin5.vocabwin5.LangSelectActivity" />
        <activity android:name="com.vocabwin5.vocabwin5.GameActivity" />
        <activity android:name="com.vocabwin5.vocabwin5.EndGameActivity" />
    </application>
</manifest>